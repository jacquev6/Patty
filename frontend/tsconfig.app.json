{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "cypress/support/commands.d.ts",
    "env.d.ts",
    "cypress/**/*.ts",
    "src/**/*.ts",
    "src/**/*.vue",
  ],
  "exclude": [],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "strict": true,

    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
